{
  "kind": "collectionType",
  "collectionName": "form_templates",
  "info": {
    "singularName": "form-template",
    "pluralName": "form-templates",
    "displayName": "Form_templates"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "string"
    },
    "company": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "AIA",
        "Vega"
      ]
    },
    "is_active": {
      "type": "boolean",
      "required": true
    },
    "form_created_at": {
      "type": "datetime",
      "required": true
    },
    "form_updated_at": {
      "type": "datetime",
      "required": true
    }
  }
}
