{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "singularName": "city",
    "pluralName": "cities",
    "displayName": "City"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "is_active": {
      "type": "boolean",
      "required": true
    },
    "city_created_at": {
      "type": "datetime",
      "required": true
    },
    "city_updated_at": {
      "type": "datetime",
      "required": true
    },
    "holidays": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::holiday.holiday",
      "mappedBy": "city"
    },
    "areas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::area.area",
      "mappedBy": "city"
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "cities"
    }
  }
}
