{
  "kind": "collectionType",
  "collectionName": "company_policies",
  "info": {
    "singularName": "company-policy",
    "pluralName": "company-policies",
    "displayName": "Company_policies"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "document": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "tags": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "required": true
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company"
    }
  }
}
