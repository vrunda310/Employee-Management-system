{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "duration_minutes": {
      "type": "integer",
      "required": true
    },
    "difficulty_level": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Beginner",
        "Intermediate",
        "Advanced"
      ]
    },
    "is_published": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "passing_score": {
      "type": "integer",
      "required": true
    },
    "certificate_template": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "course_created_at": {
      "type": "datetime",
      "required": true
    },
    "course_updated_at": {
      "type": "datetime",
      "required": true
    },
    "course_created_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "course_creators"
    },
    "course_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course-category.course-category"
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "inversedBy": "prerequisite_courses"
    },
    "prerequisite_courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "courses"
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company"
    }
  }
}
